[Unit]
Description=Denis ContextHarvester daemon
After=network.target

[Service]
Type=simple
ExecStart=/media/jotah/SSD_denis/.venv_oceanai/bin/python3 /media/jotah/SSD_denis/home_jotah/denis_unified_v1/scripts/start_harvester.py --workspace /media/jotah/SSD_denis/home_jotah/denis_unified_v1 --node nodo1
Restart=on-failure
RestartSec=10
Environment=PYTHONPATH=/media/jotah/SSD_denis/home_jotah
Environment=DISPLAY=:0

[Install]
WantedBy=default.target
