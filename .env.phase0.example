# DENIS_UNIFIED_V1_INCREMENTAL - Phase 0 defaults
DENIS_USE_QUANTUM_SUBSTRATE=false
DENIS_USE_QUANTUM_SEARCH=false
DENIS_USE_CORTEX=false
DENIS_USE_ORCHESTRATION_AUG=false
DENIS_USE_API_UNIFIED=false
DENIS_ENABLE_METAGRAPH=true
DENIS_AUTOPOIESIS_MODE=supervised

# Baseline check tuning
DENIS_BASELINE_ENDPOINTS=http://127.0.0.1:8084/health,http://127.0.0.1:8086/health,http://127.0.0.1:8000/health
DENIS_BASELINE_PORTS=8084,8086,9100,8000

# Neo4j connectivity for phase-1 execution scripts
NEO4J_URI=bolt://10.10.10.1:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=

# SSH passwords for infrastructure adapter (optional, uses sshpass)
DENIS_SSH_PASS_DEFAULT=
DENIS_SSH_PASS_192_168_1_65=

# Node definitions with Tailscale IPs
# Local network
DENIS_NODE1_HOST=10.10.10.1
DENIS_NODE2_HOST=10.10.10.2
DENIS_NODOMAC_HOST=192.168.1.65

# Tailscale IPs (auto-updated by polling daemon)
DENIS_NODE1_TAILSCALE=100.86.69.108
DENIS_NODE2_TAILSCALE=100.93.192.27
DENIS_NODOMAC_TAILSCALE=100.117.11.87

# Cortex polling daemon settings
CORTEX_POLL_INTERVAL=5
CORTEX_REDIS_URL=redis://localhost:6379/0

# Phase-11 Sprint Orchestrator
DENIS_USE_SPRINT_ORCHESTRATOR=false
DENIS_SPRINT_MAX_WORKERS=4
DENIS_SPRINT_SCAN_ROOT=/media/jotah/SSD_denis/home_jotah/denis_unified_v1
DENIS_SPRINT_STATE_DIR=/media/jotah/SSD_denis/home_jotah/denis_unified_v1/.sprint_orchestrator

# Real provider load (populate in .env, do not commit secrets)
DENIS_SPRINT_CODEX_CMD=codex
DENIS_SPRINT_CLAUDE_CMD=claude
DENIS_SPRINT_LLAMA_NODE1_MODE=celery
DENIS_SPRINT_LLAMA_NODE1_QUEUE=sprint:llama_node1
DENIS_SPRINT_LLAMA_NODE1_URL=http://10.10.10.1:8084/v1/chat/completions
DENIS_SPRINT_LLAMA_NODE2_MODE=celery
DENIS_SPRINT_LLAMA_NODE2_QUEUE=sprint:llama_node2
DENIS_SPRINT_LLAMA_NODE2_URL=http://10.10.10.2:8084/v1/chat/completions
DENIS_SPRINT_CELERY_APP=denis_crew_tasks
DENIS_SPRINT_CELERY_TASK=denis.sprint.execute
DENIS_SPRINT_CREW_QUEUE=sprint:crewai

# Denis MCP bridge (real endpoint preferred)
DENIS_SPRINT_MCP_ENABLED=false
DENIS_SPRINT_MCP_BASE_URL=http://127.0.0.1:8084
DENIS_SPRINT_MCP_TOOLS_PATH=/tools
DENIS_SPRINT_MCP_AUTH_TOKEN=
DENIS_SPRINT_MCP_ALLOW_FILE_CATALOG=false

# Sprint Event Bus (native store + optional Redis broadcast)
DENIS_SPRINT_EVENT_BUS_REDIS_ENABLED=true
DENIS_SPRINT_EVENT_BUS_CHANNEL=denis:sprint:events
DENIS_SPRINT_EVENT_BUS_REDIS_URL=redis://127.0.0.1:6379/0
DENIS_SPRINT_EVENT_LOG_ENABLED=true
DENIS_SPRINT_EVENT_LOG_PATH=

# Rasa Gate (deterministic intents first, LLM fallback second)
DENIS_USE_RASA_GATE=false
DENIS_SPRINT_RASA_URL=http://127.0.0.1:5005/model/parse
DENIS_SPRINT_RASA_TIMEOUT_SEC=5
DENIS_SPRINT_RASA_MIN_CONFIDENCE=0.85
DENIS_SPRINT_RASA_FALLBACK_PROVIDER=llama_node1

# Global registry (SQLite + optional Atlas sync)
DENIS_SPRINT_REGISTRY_DB=
DENIS_SPRINT_REGISTRY_ATLAS_ENABLED=false
DENIS_SPRINT_REGISTRY_ATLAS_URL=http://127.0.0.1:8084

# Placeholder/Stub guard (human approval required)
DENIS_SPRINT_PLACEHOLDER_GUARD_ENABLED=true
DENIS_SPRINT_PLACEHOLDER_GUARD_FAIL_CLOSED=true
DENIS_SPRINT_PLACEHOLDER_ALLOW_MARKER=denis:allow-placeholder
