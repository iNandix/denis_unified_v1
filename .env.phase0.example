# DENIS_UNIFIED_V1_INCREMENTAL - Phase 0 defaults
DENIS_USE_QUANTUM_SUBSTRATE=false
DENIS_USE_QUANTUM_SEARCH=false
DENIS_USE_CORTEX=false
DENIS_USE_ORCHESTRATION_AUG=false
DENIS_USE_API_UNIFIED=false
DENIS_ENABLE_METAGRAPH=true
DENIS_AUTOPOIESIS_MODE=supervised

# Baseline check tuning
DENIS_BASELINE_ENDPOINTS=http://127.0.0.1:8084/health,http://127.0.0.1:8086/health,http://127.0.0.1:8000/health
DENIS_BASELINE_PORTS=8084,8086,9100,8000

# Neo4j connectivity for phase-1 execution scripts
NEO4J_URI=bolt://10.10.10.1:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=

# SSH passwords for infrastructure adapter (optional, uses sshpass)
DENIS_SSH_PASS_DEFAULT=
DENIS_SSH_PASS_192_168_1_65=

# Node definitions with Tailscale IPs
# Local network
DENIS_NODE1_HOST=10.10.10.1
DENIS_NODE2_HOST=10.10.10.2
DENIS_NODOMAC_HOST=192.168.1.65

# Tailscale IPs (auto-updated by polling daemon)
DENIS_NODE1_TAILSCALE=100.86.69.108
DENIS_NODE2_TAILSCALE=100.93.192.27
DENIS_NODOMAC_TAILSCALE=100.117.11.87

# Cortex polling daemon settings
CORTEX_POLL_INTERVAL=5
CORTEX_REDIS_URL=redis://localhost:6379/0
